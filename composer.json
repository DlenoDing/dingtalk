{
    "name": "dleno/dingtalk",
    "type": "library",
    "description": "dingtalk by dleno(PHP)",
    "keywords": ["dingtalk"],
    "authors": [
        {
            "name": "dleno",
            "email": "dleno@126.com",
            "homepage": "https://www.dlenos.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "hyperf/utils": "~2.2.0",
        "hyperf/config": "~2.2.0",
        "hyperf/redis": "~2.2.0",
        "guzzlehttp/guzzle": "^6.3|^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Dleno\\DingTalk\\": "src/"
        },
        "files": [
            "src/common/Functions.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {},
    "bin": [
    ],
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1",
        "test": "phpunit --colors=always"
    }
}
